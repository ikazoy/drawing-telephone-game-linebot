{
  "name": "liff-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "sh ./bin/host-dev-env.sh",
    "deploy": "cp .env.development .env && sls deploy --stage development -v",
    "deploy-prod": "cp .env.production .env && sls deploy --stage production -v",
    "deploy-client": "yarn --cwd liff run build && sls client deploy --stage development -v",
    "deploy-client-prod": "yarn --cwd liff run build && sls client deploy --stage production -v"
  },
  "dependencies": {
    "@google-cloud/firestore": "^0.17.0",
    "@line/bot-sdk": "^6.3.0",
    "@vue/cli": "^3.1.3",
    "aws-sdk": "^2.353.0",
    "aws-serverless-express": "^3.2.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "dynamodb-update-expression": "^0.1.21",
    "express": "~4.16.0",
    "firebase-admin": "^6.0.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "pug": "2.0.0-beta11",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-finch": "^2.3.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "serverless": "^1.28.0",
    "serverless-offline": "^3.25.6"
  }
}