{
  "name": "liff-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "sh ./bin/host-dev-env.sh",
    "deploy": "cp .env.development .env && sls deploy --stage development -v",
    "build-netlify-client": "yarn --cwd liff run build && zip liff/dist -r liff/dist/",
    "deploy-netlify-client": "yarn --cwd liff run build && netlify deploy --dir=liff/dist",
    "deploy-client": "cp .env.development liff/.env && yarn --cwd liff run build && sls client deploy --stage development -v",
    "deploy-all-dev": "yarn run deploy && yarn run deploy-client",
    "deploy-prod": "cp .env.production .env && sls deploy --stage production -v",
    "deploy-client-prod": "cp .env.production liff/.env && yarn --cwd liff run build && sls client deploy --stage production -v",
    "deploy-all-prod": "yarn run deploy-prod && yarn run deploy-client-prod"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@google-cloud/firestore": "^0.17.0",
    "@line/bot-sdk": "^6.8.2",
    "@vue/cli": "^3.1.3",
    "aws-sdk": "^2.353.0",
    "aws-serverless-express": "^3.2.0",
    "body-parser": "^1.18.3",
    "circular-json": "^0.5.9",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "dynamodb-data-types": "^3.0.1",
    "dynamodb-update-expression": "^0.1.21",
    "express": "~4.16.0",
    "firebase-admin": "^6.0.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "pug": "3.0.1",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-finch": "^2.3.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "serverless": "^1.28.0",
    "serverless-offline": "^3.25.6",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.26.0",
    "webpack-node-externals": "^1.7.2"
  }
}